  height: 80rpx;
  line-height: 80rpx;
}
.projectProgress text{
  float: left;
  font-family: "PingFangSC-Regular";
  color: white;
  font-size: 28rpx;
}
.projectProgress .stageName{
  color: #0f161e;
  float: left;
  margin-left: 20rpx;
}
.projectProgress .stageTime{
  color: #aaa9ae;
  float: right;
}
.projectProgress .img{
  width: 43rpx;
  height: 36rpx;
  /*border: 1rpx solid red;*/
