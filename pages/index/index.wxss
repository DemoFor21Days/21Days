  align-items: center;
  justify-content: center;
}
.projectProgressSeeMore text{
  font-size: 24rpx;
  font-family: "PingFangSC-Regular";
  color: #4b69a8;
}
.projectProgressSeeMore .navigatorSeeMore {
  height:80rpx;
  line-height: 80rpx;
}
.projectProgressSeeMore .navigatorSeeMore .navigator {
  padding: 20rpx 30rpx;
  position: relative;
  /*display: flex;
  align-items: center;
  float: right;*/
  margin-top:5rpx;
}
.projectProgressSeeMore .navigatorSeeMore .navigator:before {
  content: " ";
  position: absolute;
  left: 30rpx;
  top: 0;
  right: 30rpx;
  height: 1px;
  border-top: 1rpx solid #4b69a8;
  color: #4b69a8;
}
.projectProgressSeeMore .navigatorSeeMore .navigator:first-child:before {
  display: none;
}
.projectProgressSeeMore .navigatorSeeMore .navigator-arrow:after {
  content: " ";
  display: inline-block;
  height: 18rpx;
  width: 18rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color: #4b69a8;
  border-style: solid;
  transform: matrix(0.71, 0.71, 0.71, -0.71, 0, 0);
  position: absolute;
  top: 50%;
  /*margin-top: -8rpx;*/
  right: 28rpx;
}
.progress text{
  font-family: "PingFangSC-Regular";
  display: block;
}
.progress .content{
  font-size: 28rpx;
  color: white;
}
.progress .static{
  font-size: 24rpx;
  color: white;
}
