  margin-top: 23rpx;
}
.projectProgress .location{
  width: 23rpx;
  height: 32rpx;
  margin-left:30rpx;
  margin-top:-30rpx;
  /*border:1rpx solid red;*/
}
.projectProgress button{
  width: 70rpx;
  height: 36rpx;
  line-height: 36rpx;
  float: left;
  padding: 0;
  font-size: 24rpx;
  font-family: "PingFangSC-Regular";
  color: #00c6ad;
  margin-left: 20rpx;
  margin-top: 23rpx;
