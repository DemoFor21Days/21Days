.order_process {
  display: flex;
  flex-wrap: nowrap;
  padding: 10rpx 10rpx 20rpx 10rpx;
  background-color: #fff;
  margin-top: 66rpx;
}

.process_wrap {
  display: flex;
  flex-direction: column;
  flex: 1;
  align-items: center;
}

.process {
  display: flex;
  align-items: center;
  width: 100%;
}

.process_icon {
  width: 20rpx;
  height: 20rpx;
}

.process_line {
  background: #eff3f6;
  flex: 1;
  height: 5rpx;
}

.process_name {
  font-size: 24rpx;
  margin-top: -66rpx;
  font-family: "PingFangSC-Regular";
}
.process_name_bottom{
  font-size: 24rpx;
  margin-top: 36rpx;
  font-family: "PingFangSC-Regular";
}
.process .transform{
  transform:rotate(90deg);
  margin-left:-64rpx;
  margin-top:54rpx;
  font-size:22rpx;
  color: #3eb85f;
}
.process .location{
  width: 23rpx;
  height: 32rpx;
  margin-left:-21rpx;
  margin-top:-30rpx;
}
.item-list .operation{
  display: flex;
  justify-content: center;
  align-items: center;
}
.operation .punchCardStatusTrue{
  height:50rpx;
  font-size:28rpx;
  line-height:50rpx;
  background-color: #f5f5f5;
  width:108rpx;
  padding:0
}
.operation .punchCardStatusFalse{
  color:white;
  background-color:#00c6ae;
  width:108rpx;
  height:50rpx;
  padding:0;
  font-size:28rpx;
  line-height:50rpx;
}
.operation .punchCardStatusTrue::after,.operation .punchCardStatusFalse::after{
  border: 0;
  border-radius:5rpx;
}
.item-list{
  position: relative;
  width: 100%;
  height: 240rpx;
  margin-left: 20rpx;
}

.item-info, .item-oper{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  /*border-bottom: 1px solid #ccc;*/
}

.item-info{
  display: flex;
  padding: 20rpx 20rpx 20rpx 0;
  z-index: 2;
  background: white;
  transition: left 0.2s ease-in-out;
}


.item-oper{
  display: flex;
  justify-content: flex-end;
}

.oper-delete{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 162rpx;
  z-index: 1;
  color: white;
  background: #fd716a;
}





















































